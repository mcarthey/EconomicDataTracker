{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=EconomicDataTracker.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "System.Net.Http.HttpClient": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "Logs/log.txt",
      "FileSizeLimitBytes": 10485760,
      "MaxRollingFiles": 5,
      "Append": true,
      "MinLevel": "Trace"
    }
  }
}
